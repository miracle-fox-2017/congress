<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Top5</title>
  </head>
  <body>
    <%= //JSON.stringify(top5) %>
    <a href="/">Back</a>
    <table border="1">
      <tr>
        <th>Total Vote</th>
        <th>Politician</th>
        <th>Voters</th>
      </tr>
      <!-- disini loop sebanyak top5 -->
      <% top5.forEach(t=>{ %>
      <tr>
        <td><%= t.total %></td>
        <td><%= t.politician %></td>
        <td>
          <ol><% t.name.forEach(tn=>{ %>
              <li><%= tn %></li>
            <% }) %>
          </ol>
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>