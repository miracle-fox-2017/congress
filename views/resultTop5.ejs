<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Polling Congres</title>
  </head>
  <body>
    <a href="/">Back</a>
    <table border="1px">
      <thead>
        <tr>
          <th>Total Vote</th>
          <th>Politician</th>
          <th>Voters</th>
        </tr>
      </thead>
      <tbody>
        <% dataTop5.forEach(data => { %>
          <tr>
            <td><%= data.TotalVote %></td>
            <td><%= data.PoliticianName %></td>
            <% let voters = data.VoterName.split(',') %>
            <td>
              <ol>
              <% voters.forEach(voter => { %>
                    <li><%= voter %></li>
              <% }) %>
              </ol>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>

  </body>
</html>
